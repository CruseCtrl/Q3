<Window x:Class="Q3Client.QueueList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:q="clr-namespace:Q3Client"
        xmlns:tb="http://www.hardcodet.net/taskbar"
        Title="QueueList" Height="600" Width="500" WindowStyle="None" WindowStartupLocation="Manual" AllowsTransparency="True" ResizeMode="NoResize"
        ShowInTaskbar="False" Topmost="True" 
        Left="5" Top="5" Focusable="False" ShowActivated="False" Icon="q3.ico">
    <Window.Background>
        <SolidColorBrush Opacity="0" Color="Black"></SolidColorBrush>
    </Window.Background>
    <StackPanel Name="QueuesPanel">
        <q:Header x:Name="Header"></q:Header>
        <tb:TaskbarIcon ToolTipText="hello world" IconSource="q3.ico"/>
    </StackPanel>
</Window>
