<Window x:Class="Q3Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" MinWidth="525" ResizeMode="NoResize">
    <Grid>
        <Button Content="Create" HorizontalAlignment="Left" Margin="156,10,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBox x:Name="QueueName" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="MessageLabel" HorizontalAlignment="Left" Height="226" Margin="10,83,0,0" VerticalAlignment="Top" Width="497"/>
        <Button x:Name="JoinQueue" Content="Join" HorizontalAlignment="Left" Margin="156,38,0,0" VerticalAlignment="Top" Width="75" Click="JoinQueue_Click"/>
        <Button Content="Activate" HorizontalAlignment="Left" Margin="236,38,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <ComboBox x:Name="QueueList" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" Width="120" SelectedIndex="0"/>
        <Button Content="Close" HorizontalAlignment="Left" Margin="316,38,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>

    </Grid>
</Window>
